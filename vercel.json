{
  "functions": {
    "server/api/sse.ts": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/api/sse",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        },
        {
          "key": "Connection",
          "value": "keep-alive"
        }
      ]
    }
  ]
}